{
    "manifest_version": 3,
    "name": "Länsförsäkringar QR Capture",
    "version": "1.0",
    "description": "Captures and broadcasts QR codes from Länsförsäkringar website",
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "host_permissions": [
        "http://localhost:3000/*",
        "*://*.lansforsakringar.se/*",
        "<all_urls>"
    ],
    "action": {
        "default_title": "Länsförsäkringar QR Capture",
        "default_icon": "icon48.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.lansforsakringar.se/*"],
            "js": ["content_lans.js"]
        }
    ]
} 